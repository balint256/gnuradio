<?xml version="1.0"?>
<!--
###################################################
##Complex Conjugate
###################################################
 -->
<block>
	<name>Complex Conjugate</name>
	<key>blocks_conjugate_cc</key>
	<import>from gnuradio import blocks</import>
	<make>blocks.conjugate_cc($enable)</make>
	<callback>set($enable)</callback>
	<param>
		<name>Enable</name>
		<key>enable</key>
		<value>True</value>
		<type>raw</type>
		<option>
			<name>Yes</name>
			<key>True</key>
		</option>
		<option>
			<name>No</name>
			<key>False</key>
		</option>
	</param>
	<sink>
		<name>in</name>
		<type>complex</type>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
	</source>
</block>
